{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "name",
    "description",
    "created_at",
    "updated_at",
    "url"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Ficção"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "Ficção"
      ],
      "pattern": "^(.*)$"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2018-11-10T18:46:45.000Z"
      ],
      "pattern": "^(.*)$"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2018-11-11T22:47:03.000Z"
      ],
      "pattern": "^(.*)$"
    },
    "url": {
      "$id": "#/properties/url",
      "type": "string",
      "title": "The Url Schema",
      "default": "",
      "examples": [
        "http://0.0.0.0:3000/categories/1.json"
      ],
      "pattern": "^(.*)$"
    }
  }
}